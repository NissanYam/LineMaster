<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#E1EEF7FA"
    tools:context=".Activities.MainActivity">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/LL"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/LeadingColor">
        <ImageView
            android:id="@+id/main_IMG_return"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="0.2"
            android:src="@drawable/ic_arrow_back_24dp"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/logout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:src="@drawable/baseline_logout_24"
            android:layout_weight="0.2"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <FrameLayout
        android:id="@+id/main_LAY"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/LL"/>
    <com.akki.circlemenu.CircleMenu
        android:id="@+id/main_circle_menu"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:menu_background_color="@array/colors"
        app:menu_close_duration="300"
        app:menu_icons="@array/icons"
        app:menu_open_duration="400"
        app:menu_orientation="vertical"
        app:menu_radius="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="20dp"/>
</RelativeLayout>